<div>
    <h1>Change your profile information below</h1>

    <form method="POST">
        {{#userInfo}}
            <div>
                <label for="first">First Name</label>
                <input
                    type="text"
                    name="first"
                    id="first"
                    value="{{this.first}}"
                />
            </div>
            <div>
                <label for="last">Last Name</label>
                <input
                    type="text"
                    name="last"
                    id="last"
                    value="{{this.last}}"
                />
            </div>
            <div>
                <label for="email">Email Address</label>
                <input
                    type="email"
                    name="email"
                    id="email"
                    value="{{this.email}}"
                />
            </div>
            <div>
                <label for="password">Password</label>
                <input type="password" name="password" id="password" />
            </div>
            <div>
                <label for="first">Age</label>
                <input type="text" name="age" id="age" value="{{this.age}}" />
            </div>
            <div>
                <label for="last">City</label>
                <input
                    type="text"
                    name="city"
                    id="city"
                    value="{{this.city}}"
                />
            </div>
            <div>
                <label for="last">URL</label>
                <input type="text" name="url" id="url" value="{{this.url}}" />
            </div>
        {{/userInfo}}
        <button>UPDATE</button>
    </form>
</div>